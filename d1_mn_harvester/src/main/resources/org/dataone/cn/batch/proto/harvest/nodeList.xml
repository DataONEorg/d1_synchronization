    <node replicate="true" synchronize="true" type="mn">
        <identifier>http://cn-rpw</identifier>
        <name>DataONESamples</name>
        <description>personal member node not widely available</description>
        <baseURL>http://cn-rpw/mn/</baseURL>
 		<services>
			<service version="0.5a" available="true">
				<name>mn_crud</name>
				<method name="get" rest="object/${GUID}" implemented="true" />
				<method name="getSystemMetadata" rest="meta/${GUID}" implemented="true" />
				</service>
			<service version="0.5a" available="true">
				<name>mn_replication</name>
				<method name="listObjects" rest="object" implemented="true"/>
				</service>
			</services>
        <synchronization>
            <schedule sec="00" min="00" hour="12" mday="*"  mon="*" wday="*" year="*" />
            <lastHarvested>2010-06-21T11:28:00Z</lastHarvested>
            <lastCompleteHarvest>2010-06-21T11:28:00Z</lastCompleteHarvest>
            </synchronization>
        <health state="up">
			<ping success="true" lastSuccess="2010-06-21T11:28:00Z"/>
			<status dateChecked="2010-06-21T11:28:00Z" success="true"/>
			</health>
        </node>

