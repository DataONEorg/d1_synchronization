<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">


<beans>

<bean id="mercuryDbHarvester" class="org.dataone.cn.mercury.Harvester">
      <property name="threads">
	      <value>${mercury3.threads}</value>
      </property>
      <property name="mercury">
	      <value>${mercury3.mercury}</value>
      </property>
      <property name="instance">
	      <value>${mercury3.instance}</value>
      </property>
      <property name="htmlurl">
	      <value>${mercury3.htmlurl}</value>
      </property>
      <property name="log">
	      <value>true</value>
      </property>
</bean>

</beans>
